
name = NodeStream Example profile
description = An example profile that shows how to extend NodeStream.
core = 7.x
theme = bartik

; NS Core is a module that contains the upgrade path for all NodeStream
; sites and some other goodies that is required for NodeStream to work.
; We use a module instead of a profile, since it we can make NodeStream
; features profile independent.
dependencies[] = ns_core

; Core
dependencies[] = menu
dependencies[] = dblog
dependencies[] = help
dependencies[] = menu
dependencies[] = taxonomy

; Contrib
dependencies[] = admin_menu
dependencies[] = admin_menu_toolbar
dependencies[] = taxonomy_menu_form
dependencies[] = configdump

; NodeStream things
dependencies[] = ns_prod_enterprise
dependencies[] = ns_prod_enterprise_office
dependencies[] = ns_prod_enterprise_user
dependencies[] = ns_prod_enterprise_client
dependencies[] = ns_prod_enterprise_blog
dependencies[] = ns_prod_enterprise_office_map
dependencies[] = ns_markdown
dependencies[] = ns_example_profile_settings
dependencies[] = ns_field_translation
dependencies[] = ns_prod_enterprise_service